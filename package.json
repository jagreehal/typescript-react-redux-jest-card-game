{
  "name": "typescript-react-redux-jest-card-game",
  "version": "1.0.0",
  "author": "Jag Reehal",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "predev": "tsc",
    "dev": "concurrently \"npm run watch:tsc\" next",
    "prestart": "rimraf pages/*.js && tsc -p tsconfig-prod.json && next build",
    "start": "next start",
    "tsc": "tsc",
    "test": "jest",
    "watch:test": "jest --watch",
    "watch:tsc": "tsc --watch",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@storybook/addon-storyshots": "^3.2.14",
    "emotion": "^8.0.10",
    "emotion-server": "^8.0.10",
    "next": "^4.1.4",
    "react": "16.1.0",
    "react-dom": "16.1.0",
    "react-emotion": "^8.0.10",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.2.14",
    "@storybook/addon-links": "^3.2.14",
    "@storybook/react": "^3.2.14",
    "@types/enzyme": "^3.1.3",
    "@types/jest": "^21.1.5",
    "@types/next": "^2.4.5",
    "@types/node": "^8.0.50",
    "@types/prettier": "^1.7.2",
    "@types/react": "^16.0.22",
    "@types/react-addons-test-utils": "^0.14.20",
    "@types/react-dom": "^16.0.3",
    "@types/react-redux": "^5.0.12",
    "@types/redux-logger": "^3.0.5",
    "@types/redux-mock-store": "^0.0.12",
    "awesome-typescript-loader": "^3.3.0",
    "babel-plugin-emotion": "^8.0.10",
    "concurrently": "^3.5.0",
    "enzyme": "^3.1.1",
    "enzyme-adapter-react-16": "1.0.4",
    "jest": "^21.2.1",
    "prettier": "^1.8.2",
    "react-addons-test-utils": "^15.6.2",
    "redux-mock-store": "^1.3.0",
    "ts-jest": "^21.2.1",
    "tslint": "^5.8.0",
    "tslint-config-airbnb": "5.3.0",
    "tslint-config-prettier": "^1.6.0",
    "tslint-react": "^3.2.0",
    "typescript": "^2.6.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "mapCoverage": true,
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testMatch": [
      "**/*.spec.(ts|tsx)"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ]
  }
}
